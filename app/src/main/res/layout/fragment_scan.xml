            app:layout_constraintHorizontal_bias="0.187"
            app:layout_constraintStart_toStartOf="@+id/Sub1"
            app:layout_constraintTop_toBottomOf="@+id/Applied"
            app:layout_constraintVertical_bias="0.197" />

        <TextView
            android:id="@+id/Sub3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-black"
            android:text="SUB3"
            android:onClick="onClick3"
            android:textColor="#000000"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.083"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Sub1"
            app:layout_constraintVertical_bias="0.193" />

        <TextView
            android:id="@+id/Sub4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-black"
            android:text="SUB4"
            android:onClick="onClick4"
            android:textColor="#000000"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/Sub2"
            app:layout_constraintTop_toBottomOf="@+id/Sub2"
            app:layout_constraintVertical_bias="0.193" />

        <TextView
            android:id="@+id/Applied"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="224dp"
            android:fontFamily="sans-serif-black"
            android:text="Subjects Applied"
            android:textColor="#000000"
            android:textSize="20sp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintVertical_bias="0.858" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>